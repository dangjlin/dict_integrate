<% provide(:title, 'Sign up') %>
<h1>查字典吧！！</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@word) do |f| %>
      <%= f.label :input_word %>
      <%= f.text_field :input_word %>
       <%= f.submit "送出查詢!!", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>
